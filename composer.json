{
    "name": "myonlinestore/commandbus",
    "type": "library",
    "description": "MyOnlineStore Commandbus Wrapper",
    "homepage": "https://github.com/MyOnlineStore/commandbus",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MyOnlineStore\\CommandBus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyOnlineStore\\CommandBus\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.0.0",
        "league/tactician": "^1.0"
    },
    "require-dev": {
        "escapestudios/symfony2-coding-standard": "^3.4",
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.0.25"
        }
    }
}
