{
    "name": "myonlinestore/commandbus",
    "type": "library",
    "description": "MyOnlineStore Commandbus Wrapper",
    "homepage": "https://github.com/MyOnlineStore/commandbus",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MyOnlineStore\\CommandBus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyOnlineStore\\CommandBus\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.0.33",
        "league/tactician": "^1.0"
    },
    "require-dev": {
        "myonlinestore/coding-standard": "^1.0",
        "phpunit/phpunit": "^6.5"
    },
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.0.33"
        }
    }
}
