{
    "name": "myonlinestore/commandbus",
    "type": "library",
    "description": "MyOnlineStore Commandbus Wrapper",
    "homepage": "https://github.com/MyOnlineStore/commandbus",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MyOnlineStore\\CommandBus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MyOnlineStore\\CommandBus\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4 | ^8.0",
        "league/tactician": "^1.1"
    },
    "require-dev": {
        "myonlinestore/coding-standard": "^3.1",
        "myonlinestore/php-devtools": "^0.2",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.18"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
